{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs",
  "projectType": "application",
  "targets": {
    "dev": {
      "command": "next dev --turbo",
      "options": {
        "cwd": "apps/docs"
      }
    },
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/docs"
      },
      "outputs": ["{projectRoot}/.next"]
    },
    "start": {
      "command": "next start",
      "options": {
        "cwd": "apps/docs"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "command": "eslint .",
      "options": {
        "cwd": "apps/docs"
      }
    }
  }
}
